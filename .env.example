# Application
APP_NAME=go-alert-web3-bnb
APP_ENV=development

# WebSocket
WS_URL=wss://stream.binance.com:9443/ws
WS_RECONNECT_INTERVAL=5s
WS_MAX_RETRIES=10

# Telegram
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here
TELEGRAM_RATE_LIMIT=30

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_POOL_SIZE=10
REDIS_LOCK_TTL=30s
REDIS_PROCESSED_TTL=5m

# Logger
LOG_LEVEL=info
LOG_FORMAT=json
LOG_OUTPUT=stdout
